#include <stdio.h>
#define PI 3.14159  // symbolic constant

int main() {
    float radius, area;

    // Input
    printf("Enter the radius of the circle: ");
    scanf("%f", &radius);

    // Calculation
    area = PI * radius * radius;

    // Output
    printf("The area of the circle is: %.2f\n", area);

    return 0;
}

